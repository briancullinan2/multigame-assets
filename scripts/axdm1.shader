//TELEPORT

textures/axdm1/teleport
{
    qer_editorimage textures/q1/teleport.tga

    surfaceparm nolightmap
    surfaceparm nonsolid
    surfaceparm nomarks

    q3map_globaltexture

    nopicmip

    {
        map textures/q1/teleport2.tga
        tcMod turb 0 0.5 0 0.05
    }
    {
        map textures/q1/teleport.tga
        tcMod turb 0 0.5 0.3 0.05
        tcMod scale 1.5 1.5
        tcmod scroll .01 .005
        blendFunc blend
        alphaGen wave sin 0.4 .1 0.4 0.4
    }
    {
        map textures/q1/teleport2.tga
        tcMod turb 0 0.5 0.3 0.025
        tcMod scale 2 2
        blendFunc blend
        alphaGen wave sin 0.4 .1 0 0.3
    }
}

//LAVA

textures/axdm1/clear_lava_400
{
	qer_editorimage textures/axdm1/eyetoeye_lava.tga	
	q3map_globaltexture
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm noimpact
	surfaceparm trans
	cull disable
	q3map_surfacelight 10000
	q3map_lightsubdivide 64
	q3map_lightimage textures/axdm1/eyetoeye_lava.tga	
	deformVertexes wave 100 sin 3 2 .1 0.1
	{
		map textures/axdm1/eyetoeye_lava.tga	
		blendfunc GL_ONE GL_ZERO
	}
	{ 
		map textures/axdm1/pool3d_3f.tga
		blendfunc GL_dst_color GL_one
		rgbgen identity
		tcmod scale .5 .5
		tcmod scroll -.05 .001
	}
	{ 
		map textures/axdm1/pool3d_3g.tga
		blendfunc GL_dst_color GL_one
		rgbgen identity
		tcmod scale .5 .5
		tcmod scroll .025 -.001
	}
}

