		surfaceparm	trans
		surfaceparm	nonsolid
		surfaceparm	fog
		surfaceparm	nolightmap

		fogparms ( 0.739129 1.000000 0.834155 ) 38000
}

textures/kvart/kvartnebo
{
qer_editorimage textures/kvart/kvartnebo.tga
surfaceparm noimpact
surfaceparm nolightmap
surfaceparm nomarks
q3map_backsplash 0 0
surfaceparm sky
q3map_surfacelight 100
q3map_sun 1.000000 0.925049 0.928483 35 225 70
skyparms env/kvart/kvart - -
	{
		map textures/skies/dimclouds.tga
                blendfunc GL_ONE GL_ONE
		tcMod scroll 0.05 0
                tcMod scale 3 2
	}
} 