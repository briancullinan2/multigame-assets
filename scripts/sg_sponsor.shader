textures/sg_sponsor/AMD_RGBr
{   
    qer_editorimage textures/sg_sponsor/AMD_RGBr.tga
    nopicmip
	{
		map textures/sg_sponsor/AMD_RGBr.tga
  	}
	{
		map $lightmap
	    	rgbGen identity
		blendfunc GL_DST_COLOR GL_ZERO
	}
	{
		map $lightmap
		tcgen environment
		tcmod scale .5 .5
	    	rgbGen wave sin .15 0 0 0
		blendfunc add
	}
}


