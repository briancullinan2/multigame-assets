textures/dm12/city
{
	surfaceparm nolightmap
	surfaceparm noimpact
	surfaceparm nomarks
	surfaceparm sky
	q3map_surfacelight 80
	q3map_sun 1 1 1 40 100 37 //R G B Intens(65) Deg Elev(30)
	skyparms env/city 128 128
}

textures/dm12/torchflame
{
	qer_editorimage textures/dm12/fire/flame_01.tga
	q3map_lightimage textures/dm12/fire/flame_01.tga	
	surfaceparm trans
	surfaceparm noimpact
	surfaceparm nolightmap
	surfaceparm nonsolid
	surfaceparm nomarks
	nopicmip
	cull none
	q3map_surfacelight 60
	q3map_backSplash 10 15 //percent distance
	q3map_cnq3_depthFade 1 0
	deformVertexes autosprite2
	{
		clampmap textures/dm12/fire/flameball.jpg
		blendFunc add
		rgbGen wave sin 0.25 0.1 0 0.4
		tcMod transform 2 0 0 1 -0.5 0
	}
	{
		animMap 32 textures/dm12/fire/flame_01.tga textures/dm12/fire/flame_02.tga textures/dm12/fire/flame_03.tga textures/dm12/fire/flame_04.tga textures/dm12/fire/flame_05.tga textures/dm12/fire/flame_06.tga textures/dm12/fire/flame_07.tga textures/dm12/fire/flame_08.tga
		blendFunc add
		rgbGen wave square 0.5 0.5 0 2
	}
	{
		animMap 32 textures/dm12/fire/flame_09.tga textures/dm12/fire/flame_10.tga textures/dm12/fire/flame_11.tga textures/dm12/fire/flame_12.tga textures/dm12/fire/flame_13.tga textures/dm12/fire/flame_14.tga textures/dm12/fire/flame_15.tga textures/dm12/fire/flame_16.tga
		blendFunc add
		rgbGen wave square 0.5 0.5 0.5 2
	}
}

