//jumppad energy flow:
textures/dm12/jp/alpha_vert
{
    qer_editorimage textures/dm12/jp/alpha_vert.tga
    qer_trans .5
    surfaceparm nonsolid
    surfaceparm nodraw
    surfaceparm trans
    q3map_alphaMod volume    <-sets a brush to alter the vertices it overlaps
    q3map_alphaMod set 0     <-sets the value of the v alpha given
}

// 2 groups of cyan electric beams with 3 beams per group. each group goes up, rotates and changes intensity at different speeds
// 2 groups of white trails. each group has its own trail size and speed
textures/dm12/jp/01
{
	qer_editorimage textures/dm12/jp/01.tga
	qer_trans 0
	surfaceparm nodlight
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	cull none
	nopicmip
	deformVertexes wave 200 sin 0 2 0 0.37
	q3map_lightimage textures/dm12/jp/01.tga
	q3map_surfacelight 65 //85
	q3map_backSplash 10 25

	{
		animMap 16 textures/dm12/jp/01.tga textures/dm12/jp/02.tga textures/dm12/jp/03.tga textures/dm12/jp/04.tga textures/dm12/jp/05.tga textures/dm12/jp/06.tga textures/dm12/jp/07.tga textures/dm12/jp/08.tga
		blendFunc GL_SRC_ALPHA GL_ONE
		rgbGen wave sin 0.8 0.2 0 0.3
		alphaGen vertex
		tcMod transform 0 -0.5 0.5 0 0 0
		tcMod scroll -0.7 0.17
	}
	{
		animMap 16 textures/dm12/jp/01.tga textures/dm12/jp/02.tga textures/dm12/jp/03.tga textures/dm12/jp/04.tga textures/dm12/jp/05.tga textures/dm12/jp/06.tga textures/dm12/jp/07.tga textures/dm12/jp/08.tga
		blendFunc GL_SRC_ALPHA GL_ONE
		rgbGen wave sin 0.8 0.2 0 0.5
		alphaGen vertex
		tcMod transform 0 -0.5 0.5 0 0 0.5
		tcMod scroll -1.1 -0.13
	}
	{
		map textures/dm12/jp/omfgstars.jpg
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen vertex
		tcMod scale 2 0.125
		tcMod scroll 0 -0.23
	}
	{
		map textures/dm12/jp/omfgstars.jpg
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen vertex
		tcMod scale 1 0.0625
		tcMod scroll 0 -0.17
	}
}

