//slime effects:
textures/dm12/moats_g
{
	qer_alphafunc greater 0.5
	qer_trans 0.6
	qer_nocarve
	qer_editorimage textures/dm12/moats_g.jpg
	surfaceparm trans	
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm nodlight
	surfaceparm nolightmap
	cull none
	nopicmip
	deformVertexes bulge 128 1 2	
	{
		map textures/dm12/moats_g.jpg
		blendFunc GL_SRC_ALPHA GL_ONE
		rgbGen vertex
        	tcMod turb 0 .07 0 .22
		tcMod scroll 0 .12
	}
}

textures/dm12/slime
{
	qer_editorimage textures/dm12/slime.jpg
	qer_nocarve
	qer_trans 0.9
	q3map_globaltexture
	surfaceparm noimpact
	surfaceparm slime
	sort 6
	q3map_lightmapSampleOffset -8
	q3map_surfacelight 100
	q3map_backSplash 0 4
	q3map_cnq3_depthfade 10 0
	tesssize 32
	nopicmip
	cull disable
	deformVertexes wave 40 sin 0 2.5 0 0.05
    {
        	map textures/dm12/slime.jpg
        	blendFunc blend
        	rgbGen const ( 0.35 0.35 0.35 )
        	tcMod turb 0 0.02 0 0.17
    }
}

