textures/enclave_ship/ceiling005
{
	qer_editorimage textures/enclave_ship/ceiling005
	q3map_surfacelight 1000
	q3map_backSplash 0.5 8
    {
        map $lightmap
    }
    {
        map textures/enclave_ship/ceiling005
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/enclave_ship/ceiling005_glow
        blendFunc GL_ONE GL_ONE
	glow
    }
}

